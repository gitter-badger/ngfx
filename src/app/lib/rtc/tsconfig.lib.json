{
    "extends": "../../../../tsconfig.json",
    "compilerOptions": {
        "baseUrl": "./src",
        "rootDir": "./../../../../tmp",
        "sourceMap": false,
        "declaration": true,
        "stripInternal": true,
        "allowSyntheticDefaultImports": true,
        "noImplicitAny": false,
        "removeComments": true,
        "allowUnreachableCode": false,
        "skipLibCheck": true
